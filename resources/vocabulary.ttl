@prefix :        <https://covidtesty.vse.cz/vocabulary#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix ncit:    <http://purl.obolibrary.org/obo/NCIT_> . 
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix reg:     <http://purl.org/linked-data/registry#> .
@prefix schema:  <http://schema.org/> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix vann:    <http://purl.org/vocab/vann/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

<https://covidtesty.vse.cz/vocabulary> a owl:Ontology ;
  rdfs:label "Antigen covid test vocabulary"@en ;
  dcterms:creator <http://mynarz.net/#jindrich>,
                  [
                    a schema:Person ;
                    schema:email "svatek@vse.cz"
                  ], 
                  [
                    a schema:Person ;
                    schema:email "hank05@vse.cz"
                  ] ;
  vann:preferredNamespacePrefix "act" ;
  vann:preferredNamespaceUri : .


##########################################################################
#NCIT imported classes: owl:imports ncit:C41393, ncit:C41394, ncit:C41395 
##########################################################################

###  http://purl.obolibrary.org/obo/NCIT_C25466
ncit:C25466 rdf:type owl:Class ;
            rdfs:label "Criterion"^^xsd:string .

###  http://purl.obolibrary.org/obo/NCIT_C41393
ncit:C41393 rdf:type owl:Class ;
            rdfs:subClassOf ncit:C25466 ;
            <http://purl.obolibrary.org/obo/IAO_0000115> "A qualitative characteristic or quantitative parameter used to provide a reliable and valid estimate of the degree to which patients who were diagnosed with particular condition or disease, match the traditional (prototypical) conception of this disease or condition."^^xsd:string ;
            ncit:NHC0 "C41393"^^xsd:string ;
            ncit:P106 "Functional Concept"^^xsd:string ;
            ncit:P108 "Diagnostic Accuracy Assessment Criterion"^^xsd:string ;
            ncit:P207 "C1511875"^^xsd:string ;
            ncit:P366 "Diagnostic_Accuracy_Assessment_Criterion"^^xsd:string ;
            <http://www.geneontology.org/formats/oboInOwl#hasExactSynonym> "Diagnostic Accuracy Assessment Criteria"^^xsd:string ,
                                                                           "Diagnostic Accuracy Assessment Criterion"^^xsd:string ,
                                                                           "Diagnostic Test Quality Assessment Criteria"^^xsd:string ,
                                                                           "Diagnostic Test Quality Assessment Criterion"^^xsd:string ;
            rdfs:label "Diagnostic Accuracy Assessment Criterion"^^xsd:string .

###  http://purl.obolibrary.org/obo/NCIT_C41394
ncit:C41394 rdf:type owl:Class ;
            rdfs:subClassOf ncit:C41393 ;
            <http://purl.obolibrary.org/obo/IAO_0000115> "The probability that a test will produce a true postive result when used on effected subjects as compared to a reference or \"gold standard\". The sensitivity of a test can be determined by calculating: number of true positive results divided by the sum of true positive results plus number of false negative results."^^xsd:string ;
            ncit:NHC0 "C41394"^^xsd:string ;
            ncit:P106 "Functional Concept"^^xsd:string ,
                      "Quantitative Concept"^^xsd:string ;
            ncit:P108 "Diagnostic Sensitivity"^^xsd:string ;
            ncit:P207 "C1511883"^^xsd:string ;
            ncit:P325 "The frequency with which a test yields a true positive result among individuals who actually have the disease or the gene mutation in question. A test with high sensitivity has a low false-negative rate and thus does a good job of correctly identifying affected individuals."^^xsd:string ;
            ncit:P366 "Diagnostic_Sensitivity"^^xsd:string ;
            <http://www.geneontology.org/formats/oboInOwl#hasExactSynonym> "Diagnostic Sensitivity"^^xsd:string ,
                                                                           "Sensitivity"^^xsd:string ,
                                                                           "Sensitivity of Diagnostic Test"^^xsd:string ,
                                                                           "sensitivity"^^xsd:string ;
            rdfs:label "Diagnostic Sensitivity"^^xsd:string .

###  http://purl.obolibrary.org/obo/NCIT_C41395
ncit:C41395 rdf:type owl:Class ;
            rdfs:subClassOf ncit:C41393 ;
            <http://purl.obolibrary.org/obo/IAO_0000115> "The probability that a test will produce a true negative result when used on non-effected subjects as compared to a reference or \"gold standard\". The specificity of a test can be determined by calculating: number of true negative results divided by the sum of true negative results plus number of false positive results."^^xsd:string ;
            ncit:NHC0 "C41395"^^xsd:string ;
            ncit:P106 "Functional Concept"^^xsd:string ,
                      "Quantitative Concept"^^xsd:string ;
            ncit:P108 "Diagnostic Specificity"^^xsd:string ;
            ncit:P207 "C1511884"^^xsd:string ;
            ncit:P325 "The frequency with which a test yields a true negative result among individuals who do not have the disease or the gene mutation in question. A test with high specificity has a low false-positive rate and thus does a good job of correctly classifying unaffected individuals."^^xsd:string ;
            ncit:P366 "Diagnostic_Specificity"^^xsd:string ;
            <http://www.geneontology.org/formats/oboInOwl#hasExactSynonym> "Diagnostic Specificity"^^xsd:string ,
                                                                           "Specificity"^^xsd:string ,
                                                                           "Specificity of Diagnostic Test"^^xsd:string ,
                                                                           "specificity"^^xsd:string ;
            rdfs:label "Diagnostic Specificity"^^xsd:string .


##################################################################
# Classes
##################################################################

:AntigenCovidTest a rdfs:Class ;
  rdfs:label "Antigen covid test"@en ;
  rdfs:subClassOf schema:Product, schema:MedicalDevice ;
  rdfs:isDefinedBy <https://covidtesty.vse.cz/vocabulary> .

:PeiDegree a rdfs:Class ;
  rdfs:label "PEI sensitivity degree"@en ;
  rdfs:subClassOf ncit:C41393 ; # = Diagnostic Accuracy Assessment Criterion <https://www.ebi.ac.uk/ols/ontologies/ncit/terms?iri=http%3A%2F%2Fpurl.obolibrary.org%2Fobo%2FNCIT_C41393>
  rdfs:isDefinedBy <https://covidtesty.vse.cz/vocabulary> .

:PeiTotalSensitivity a rdfs:Class ;
  rdfs:label "Total sensitivity according to PEI, counted as weighted average"@en ;
  rdfs:comment "Total sensitivity is the overall sensitivity of a test in evaluation done by Paul-Ehrlich Institute. This value is counted as weighted average from the results of sensitivities in three viral load categories (high, medium and low viral load)."@en ;
  rdfs:subClassOf ncit:C41393 ;
  rdfs:isDefinedBy <https://covidtesty.vse.cz/vocabulary> .


##################################################################
# Properties
##################################################################

:biospecimenCollectionMethod a owl:ObjectProperty ;
  rdfs:label "Biospecimen collection method"@en ;
  rdfs:domain :AntigenCovidTest ;
  rdfs:range skos:Concept ;
  rdfs:seeAlso ncit:C70700 ; # = Biospecimen collection method <https://www.ebi.ac.uk/ols/ontologies/ncit/terms?iri=http%3A%2F%2Fpurl.obolibrary.org%2Fobo%2FNCIT_C70700>
  rdfs:isDefinedBy <https://covidtesty.vse.cz/vocabulary> .

:coronavirusVariant a owl:ObjectProperty ;
  rdfs:label "Coronavirus variant"@en ;
  rdfs:range skos:Concept ;
  rdfs:isDefinedBy <https://covidtesty.vse.cz/vocabulary> .

:peiSensitivityCategory a owl:ObjectProperty ;
  rdfs:label "PEI sensitivity category"@en ;
  rdfs:domain ncit:C41394 ; # = Diagnostic sensitivity <https://www.ebi.ac.uk/ols/ontologies/ncit/terms?iri=http%3A%2F%2Fpurl.obolibrary.org%2Fobo%2FNCIT_C41394>
  rdfs:range skos:Concept ;
  rdfs:isDefinedBy <https://covidtesty.vse.cz/vocabulary> .

:peiSensitivityDegree a owl:ObjectProperty ;
  rdfs:label "PEI sensitivity degree"@en ;
  rdfs:domain ncit:C41393 ;
  rdfs:range skos:Concept ;
  rdfs:isDefinedBy <https://covidtesty.vse.cz/vocabulary> .

:ssiSensitivityCategory a owl:ObjectProperty ;
  rdfs:label "SSI sensitivity category"@en ;
  rdfs:domain ncit:C41394 ;
  rdfs:range skos:Concept ;
  rdfs:isDefinedBy <https://covidtesty.vse.cz/vocabulary> .

:ssiSensitivityDegree a owl:ObjectProperty ;
  rdfs:label "SSI sensitivity degree"@en ;
  rdfs:domain ncit:C41394 ;
  rdfs:range skos:Concept ;
  rdfs:isDefinedBy <https://covidtesty.vse.cz/vocabulary> .


##################################################################
# Code lists
##################################################################

<https://covidtesty.vse.cz/data/concept-scheme/registration-statuses> a skos:ConceptScheme ;
  rdfs:label "Registration statuses"@en ;
  rdfs:isDefinedBy <https://covidtesty.vse.cz/vocabulary> .

<https://covidtesty.vse.cz/data/concept/registration-statuses/status-approved> a skos:Concept ;
  skos:prefLabel "Approved"@en ;
  skos:exactMatch reg:statusValid ;
  skos:inScheme <https://covidtesty.vse.cz/data/concept-scheme/registration-statuses> ;
  rdfs:isDefinedBy <https://covidtesty.vse.cz/vocabulary> .

<https://covidtesty.vse.cz/data/concept/registration-statuses/status-not-approved> a skos:Concept ;
  skos:prefLabel "Not approved"@en ;
  skos:exactMatch reg:statusInvalid ;
  skos:inScheme <https://covidtesty.vse.cz/data/concept-scheme/registration-statuses> ;
  rdfs:isDefinedBy <https://covidtesty.vse.cz/vocabulary> .

<https://covidtesty.vse.cz/data/concept/registration-statuses/status-warning> a skos:Concept ;
  skos:prefLabel "Warning"@en ;
  skos:inScheme <https://covidtesty.vse.cz/data/concept-scheme/registration-statuses> ;
  rdfs:isDefinedBy <https://covidtesty.vse.cz/vocabulary> .

#SSI code list
<https://covidtesty.vse.cz/data/concept-scheme/stupen-citlivosti-ssi> a skos:ConceptScheme ;
  rdfs:label "Sensitivity according the SSI"@en ;
  rdfs:isDefinedBy <https://covidtesty.vse.cz/vocabulary> .

<https://covidtesty.vse.cz/data/concept/stupen-citlivosti-ssi/star> a skos:Concept ;
  skos:prefLabel "Positive"@en ;
  skos:inScheme <https://covidtesty.vse.cz/data/concept-scheme/stupen-citlivosti-ssi> ;
  rdfs:isDefinedBy <https://covidtesty.vse.cz/vocabulary> .

<https://covidtesty.vse.cz/data/concept/stupen-citlivosti-ssi/half-star> a skos:Concept;
  skos:prefLabel "Weak positive"@en ;
  skos:inScheme <https://covidtesty.vse.cz/data/concept-scheme/stupen-citlivosti-ssi> ;
  rdfs:isDefinedBy <https://covidtesty.vse.cz/vocabulary> .

<https://covidtesty.vse.cz/data/concept/stupen-citlivosti-ssi/cross> a skos:Concept;
  skos:prefLabel "Negative"@en ;
  skos:inScheme <https://covidtesty.vse.cz/data/concept-scheme/stupen-citlivosti-ssi> ;
  rdfs:isDefinedBy <https://covidtesty.vse.cz/vocabulary> .


